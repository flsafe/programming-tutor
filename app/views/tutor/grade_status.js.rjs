if @status == :job_in_progrress
  page.replace_html :message, 'grading...'
  page << "keepPollingGrade = true"
elsif @status == :job_done
  page.replace_html :message, 'done grading!' if @grade_sheet
  page << "keepPollingGrade = false"
  page.redirect_to :action=>:show_grade_sheet, :id=>@grade_sheet
elsif @status == :job_error
  page.replace_html :message, @result.error_message if @result
  page << "keepPollingGrade = false"
end