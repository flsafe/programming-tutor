<h1>Editing feed_back_comment</h1>

<% form_for(@feed_back_comment) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :comment %><br />
    <%= f.text_field :comment %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @feed_back_comment %> |
<%= link_to 'Back', feed_back_comments_path %>